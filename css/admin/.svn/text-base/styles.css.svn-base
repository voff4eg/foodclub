/*List*/
ul {
	margin:0;
	padding:0;}
ul li {list-style-type:none;}
/*_List_*/

/*Form*/
form {}
input.text {
	font-size:11pt;
	font-family:Arial, Helvetica, sans-serif;
	padding:14px 10px 1px 10px;
	height:32px;}
.button {margin-top:20px;}
select {
	height:28px;
	background:#f9f9f9;}
.form_field {margin-bottom:15px;}
.form_field h5 {margin:0 0 1px 0;}
.form_field h5 span {visibility:hidden;}
.attention {color:#ff0000;}
.no_text {visibility:hidden;}
div.input_file {height:30px;}
div.file_name {padding:0;}
/*_Form_*/

/*Elements*/
#content div.body {
	width:100%;
	margin-right:0;
	float:none;}
#recipe_search div.search_field .button {margin-top:0;}
/*_Elements_*/

/*Dish-form*/
#dish_description {
	padding:20px 0;
	overflow:hidden;}
#dish_description .dish_properties {
	float:left;
	width:440px;}
#dish_description .dish_properties input.text {width:410px;}
#dish_description .dish_properties textarea {
	width:410px;
	height:130px;}
#dish_description .dish_parents {
	float:left;
	margin-left:60px;}
#dish_description .dish_parents select {width:300px;}
#dish_description .file_name {
	position:relative;
	margin:4px 0 0 16px;}
#dish_description .file_name * {font-size:10pt;}
#dish_description .file_name .img_icon {
	position:absolute;
	top:5px;
	left:-10px;
	width:6px;
	height:8px;
	background:url(/images/admin/icons/img.gif) no-repeat;}
#dish_description .file_name a {}
#dish_description .file_name .file_size {margin-left:5px;}
#dish_description .file_name .delete_icon {
	background:url(/images/admin/icons/file_delete.gif) no-repeat left top;
	margin:0 0 0 8px;
	cursor:pointer;}
#dish_description .file_name img.attention {
	background-position:left -7px;
	margin:0 0 0 8px;}

#dish_stages {
	margin:0 -15px 0 -30px;
	padding:20px 30px;
	background-color:#f1f1f1;}
#dish_stages h2 {}
#dish_stages div.body {}
#dish_stages .stages_block {
	width:440px;
	float:left;}
#dish_stages .stage {
	padding:15px 0;
	background:url(/images/admin/border.gif) repeat-x left bottom;}
#dish_stages .stage .delete_icon {position:relative;}
#dish_stages .stage .delete_icon div {
	position:absolute;
	top:96px;
	left:836px;
	background:url(/images/admin/icons/delete.gif) no-repeat left top #f1f1f1;
	width:33px;
	height:33px;
	cursor:pointer;}
#dish_stages .stage div.attention div {background-position:left -33px;}
#dish_stages .description {
	width:440px;
	float:left;}
#dish_stages .description input.text {width:430px;}
#dish_stages .description textarea {
	width:430px;
	height:150px;}
#dish_stages .description .file_name {
	position:relative;
	margin:4px 0 0 16px;}
#dish_stages .description .file_name * {font-size:10pt;}
#dish_stages .description .file_name .img_icon {
	position:absolute;
	top:5px;
	left:-10px;
	width:6px;
	height:8px;
	background:url(/images/admin/icons/img.gif) no-repeat;}
#dish_stages .description .file_name a {}
#dish_stages .description .file_name .file_size {margin-left:5px;}
#dish_stages .description .file_name .delete_icon {
	background:url(/images/admin/icons/file_delete.gif) no-repeat left top;
	margin:0 0 0 8px;
	cursor:pointer;}
#dish_stages .description .file_name .attention {background-position:left -7px;}
#dish_stages .ingredient {
	float:left;
	margin-left:60px;}
#dish_stages .ingredient li {margin:2px 0 5px 0;}
#dish_stages .ingredient .name {}
#dish_stages .ingredient input.text {
	width:35px;
	margin-left:10px;}
#dish_stages .ingredient .unit {
	margin-left:10px;
	color:#999999;}
#dish_stages .ingredient li.attention .name {color:#ff0000;}
#dish_stages .ingredient li.attention .unit {color:#ff0000;}
#dish_stages .ingredient .choose {margin-top:10px;}
#content .button {text-align:center;}
#content #dish_stages .button {
	text-align:left;}

.input_file {
	height:25px;
	position:relative;}
#dish_description .input_file input.text, #dish_stages .input_file input.text, .input_file input.text {
	position: absolute;
	width: 219px;
	margin-left: -140px;
	cursor: default;
	height: 21px;
	z-index: 2;
	filter: alpha(opacity: 0);
	opacity: 0;}
.input_file .blocker {
	position: absolute;
	z-index: 3;
	width: 150px;
	height: 21px;
	background: url(/images/admin/transparent.gif);
	margin-left: -155px;}
.input_file .browse_button {
	position: absolute;
	z-index: 1;
	width: 79px;
	height: 21px;
	float: left;}
.input_file .browse_button input {
	width: 79px;
	height: 21px;}
div.input_file .new_file_name {
	position: absolute;
	height: 15px;
	margin-left: 90px;
	font-family: Verdana;
	font-size: 8pt;
	color: Gray;
	margin-top: 2px;
	padding-top: 1px;
	padding-left: 19px;}
/*_Dish-form_*/

/*Top_layers*/
#top_layer {
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:100%;
	background-color:none;
	filter:progid:DXImageTransform.Microsoft.Alpha(opacity=70); /* IE 5.5+*/
	-moz-opacity: 0.7; /* Mozilla 1.6 */
	-khtml-opacity: 0.7; /* Konqueror 3.1, Safari 1.1 */
	opacity: 0.7; /* CSS3 - Mozilla 1.7b +, Firefox 0.9 +, Safari 1.2+, Opera 9 */
	z-index:10;}
#top_layer iframe {}/*IE6...*/

#stage_ingredients_list_layer {
	position:absolute;
	top:50%;
	left:50%;
	margin:-390px 0 0 -300px;
	width:600px;
	z-index:11;}
#stage_ingredients_list_layer a {text-decoration:none;}
#stage_ingredients_list_layer .relative {position:relative;}
#stage_ingredients_list_layer ul li {list-style-type:none;}
#stage_ingredients_list_layer div.close_icon {
	position:absolute;
	top:20px;
	right:20px;
	cursor:pointer;
	width:11px;
	height:11px;
	background:url(/images/admin/icons/close.gif) no-repeat;}
#stage_ingredients_list_layer div.padding {
	padding:20px;
	position:absolute;
	top:0;
	left:0;
	background-color:#ffffff;}
#stage_ingredients_list_layer #stage_ingredients_group {
	width:200px;
	margin-right:15px;
	float:left;}
#stage_ingredients_list_layer #stage_ingredients_group ul {margin-top:13px;}
#stage_ingredients_list_layer #stage_ingredients_group ul li {margin-bottom:8px;}
#stage_ingredients_list_layer #stage_ingredients_group ul li span.name {display:none;}
#stage_ingredients_list_layer #stage_ingredients_group ul li.act span.name {display:inline;}
#stage_ingredients_list_layer #stage_ingredients_group ul li.act a {display:none;}
#stage_ingredients_list_layer #stage_ingredients_group .items {
	font-size:9pt;
	font-style:italic;
	color:#9a9a9a;
	text-transform:lowercase;
	margin-left:8px;}
#stage_ingredients_list_layer #stage_ingredients_group .items ul {
	padding:0;
	margin:0;
	display:inline;}
#stage_ingredients_list_layer #stage_ingredients_group .items li {
	margin:0;
	padding:0;
	font-size:9pt;
	font-style:italic;
	color:#9a9a9a;
	text-transform:lowercase;
	display:inline;}
#stage_ingredients_list_layer #stage_ingredients_list {
	float:left;
	width:320px;
	border-left:1px solid #f4f4f4;
	padding:0 0 0 20px;}
#stage_ingredients_list_layer #stage_ingredients_list .column {
	width:300px;
	margin-right:10px;
	float:left;}
#stage_ingredients_list_layer #stage_ingredients_list h2 {margin-bottom:10px;}
#stage_ingredients_list_layer #stage_ingredients_list ul {
	padding-left:20px;}
#stage_ingredients_list_layer #stage_ingredients_list ul li {
	margin-bottom:8px;
	font-size:10pt;
	list-style-image:url(/images/empty_marker.gif);}
#stage_ingredients_list_layer #stage_ingredients_list ul li a {font-size:10pt;}
#stage_ingredients_list_layer #stage_ingredients_list ul li.selected {list-style-image:url(/images/admin/marker.gif);}
#stage_ingredients_list_layer #stage_ingredients_list ul li.selected a {color:#000000;}
#stage_ingredients_list_layer #stage_ingredients_list ul li.selected a:hover {color:#666666;}
#stage_ingredients_list_layer .button {
	text-align:center;
	margin-top:10px;
	height:38px;
	background:url(/images/button_bg.gif) repeat-x;
	color:#ffffff;
	font-size:18pt;
	font-family:"Times New Roman", Times, serif;
	cursor:pointer;
	padding-top:11px;
	width:579px;}
/*_Top_layers_*/

/*Recipes*/
#cooking_list {
	margin-top:26px;
	float:left;
	width:220px;}
#cooking_list a {text-decoration:none;}
#cooking_list ul {
	margin-top:4px;
	padding-right:20px;
	border-right:1px solid #f4f4f4;}
#cooking_list ul h3 {margin-bottom:9px;}
#cooking_list ul li {
	margin-bottom:5px;
	color:#cccccc;}
#cooking_list ul li.act a {display:none;}
#cooking_list ul li span {display:none; background-color:#CCCCCC;}
#cooking_list ul li.act span {display:inline;}
.dishes_list {
	margin:26px 0 0 75px;
	float:left;
	display:none;}
.dishes_list .right_column {
	float:left;
	width:255px;
	margin-left:80px;}
.dishes_list .left_column {
	float:left;
	width:255px;}
.dishes_list a {text-decoration:none;}
.dishes_list ul {margin:4px 0 14px 0;}
.dishes_list ul h3 {margin-bottom:9px;}
.dishes_list ul li {
	margin-bottom:5px;
	color:#cccccc;}
/*_Recipes_*/

/*Bitrix*/
div.top-panel * {
	font-size:8pt;
	color:#333333;}
/*_Bitrix_*/