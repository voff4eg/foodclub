	width:255px;
	margin-left:80px;}
.dishes_list .left_column {
	float:left;
	width:255px;}
.dishes_list a {text-decoration:none;}
.dishes_list ul {margin:4px 0 14px 0;}
.dishes_list ul h3 {margin-bottom:9px;}
.dishes_list ul li {
	margin-bottom:5px;
	color:#cccccc;}
